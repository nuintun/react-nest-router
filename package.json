{
  "name": "react-nest-router",
  "version": "0.0.0-dev-1",
  "description": "Declarative routing for React.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "typings/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuintun/react-nest-router.git"
  },
  "keywords": [
    "react",
    "router",
    "history"
  ],
  "author": "nuintun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuintun/react-nest-router/issues"
  },
  "homepage": "https://github.com/nuintun/react-nest-router#readme",
  "scripts": {
    "tests": "node tests",
    "lint": "tsc --noEmit",
    "clean": "node tools/clean.js",
    "prepublishOnly": "npm run build",
    "build:cjs": "rollup -c tools/rollup.cjs.js",
    "build:esm": "rollup -c tools/rollup.esm.js",
    "format": "prettier --write tools src cjs esm tests typings",
    "build": "npm run clean && npm run build:main && npm run format",
    "build:main": "npm run build:cjs && npm run build:esm && npm run build:typings",
    "build:typings": "tsc --declaration --emitDeclarationOnly --declarationDir typings"
  },
  "dependencies": {
    "history": "^5.3.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "magic-string": "^0.26.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "typescript": "^4.6.2"
  }
}