{
  "name": "react-nest-router",
  "version": "0.19.20",
  "description": "Declarative routing for React.",
  "type": "module",
  "sideEffects": false,
  "main": "cjs/index.cjs",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.cjs",
    "types": "./types/index.d.ts"
  },
  "files": [
    "cjs",
    "esm",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuintun/react-nest-router.git"
  },
  "keywords": [
    "react",
    "router",
    "history",
    "location",
    "navigator"
  ],
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuintun/react-nest-router/issues"
  },
  "homepage": "https://github.com/nuintun/react-nest-router#readme",
  "scripts": {
    "test": "node tests",
    "lint": "tsc --noEmit",
    "clean": "node tools/clean.js",
    "prepublishOnly": "npm run build",
    "build:cjs": "rollup -c tools/rollup.cjs.js",
    "build:esm": "rollup -c tools/rollup.esm.js",
    "format": "prettier --write . --ignore-path .prettierignore",
    "build": "npm run clean && npm run build:main && npm run format",
    "build:main": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir types && node tools/fix-types.js"
  },
  "dependencies": {
    "tslib": "^2.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "magic-string": "^0.30.12",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.3",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  }
}
